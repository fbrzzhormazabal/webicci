<%= form_with(model: @student, local: true) do |form| %>

    <h1 class="title">Creacion de Estudiantes</h1>
    <div>
    <%= form.label :Name %>
    <%= form.text_field :Name %>
    </div>
    <div>
    <%= form.label :Mother_Name %>
    <%= form.text_field :Mother_Name %>
    </div>
    <div>
    <%= form.label :Father_Name %>
    <%= form.text_field :Father_Name %>
    </div>
    <div>
    <%= form.label :Email %>
    <%= form.text_field :Email %>
    </div>
    <div>
    <%= form.label :Run %>
    <%= form.text_field :Run %>
    </div>
    <div>
    <%= form.label :Dv %>
    <%= form.text_field :Dv %>
    </div>
    <div>
    <%= form.label :Telefono %>
    <%= form.text_field :Telefono %>
    </div>
    <div>
    <%= form.label :Birthday %>
    <%= form.text_field :Birthday %>
    </div>
    <div>
    <%= form.label :Year_Ingre %>
    <%= form.text_field :Year_Ingre %>
    </div>
    <div>
    <%= form.label :Password %>
    <%= form.text_field :Password %>
    </div>
        
    <div>
    <%= form.submit %>
    </div>

    <div class="col-md-10 col-lg-6 col-xl-7 d-flex align-items-center order-1 order-lg-2">
        <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-registration/draw1.webp" class="img-fluid" alt="Sample image">
    </div>

<%end%>