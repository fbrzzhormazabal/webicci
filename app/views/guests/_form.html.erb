<%= form_with(model: @guest, local: true) do |form| %>

    <div>
    <%= form.label :Name %>
    <%= form.text_field :Name %>
    </div>
    <div>
    <%= form.label :Email %>
    <%= form.text_field :Email %>
    </div>
    <div>
    <%= form.label :Run %>
    <%= form.text_field :Run %>
    </div>
    <div>
    <%= form.label :Dv %>
    <%= form.text_field :Dv %>
    </div>
    <div>
    <%= form.label :Password %>
    <%= form.text_field :Password %>
    </div>
        
    <div>
    <%= form.submit %>
    </div>

<%end%>